{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_display_name}",
	"license": "${mod_license}",
	"icon": "assets/${mod_id}/icon.png",
	"environment": "*",
	"description": "${mod_description}",
	"authors": [
		"${mod_author}"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/wirelessredstone",
		"sources": "https://github.com/Razzokk/WirelessRedstone",
		"issues": "https://github.com/Razzokk/WirelessRedstone/issues"
	},
	"entrypoints": {
		"main": [
			"rzk.wirelessredstone.WirelessRedstone"
		],
		"client": [
			"rzk.wirelessredstone.WirelessRedstoneClient"
		],
		"fabric-datagen": [
			"rzk.wirelessredstone.datagen.DataGenerators"
		],
		"modmenu": [
			"rzk.wirelessredstone.WRModMenu"
		]
	},
	"mixins": [
		"wirelessredstone.mixins.json"
	],
	"depends": {
		"java": ">=${java_version}",
		"minecraft": "~${mc_version}",
		"fabricloader": ">=${loader_version}",
		"fabric-api": ">=${fabric_api_version}"
	},
	"suggests": {
		"cloth-config": ">=${cloth_config_version}",
		"modmenu": ">=${modmenu_version}"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/4f2sFD53XK"
			}
		}
